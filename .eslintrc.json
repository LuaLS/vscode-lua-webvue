{
  "root": true,
  "extends": [
    "plugin:vue/vue3-essential",
    "eslint:recommended",
    "@vue/eslint-config-typescript",
    "@vue/eslint-config-prettier"
  ],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "linebreak-style": ["warn", "auto"],
    "vue/multi-word-component-names": ["off"],
    "prettier/prettier": [
      "warn",
      {
        "endOfLine": "crlf"
      }
    ]
  },
  "ignorePatterns": ["assets"]
}
